{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "store-press/sp-all-products",
	"version": "1.0.0",
	"title": "SP All Products",
	"category": "store-press-blocks",
	"icon": "layout",
	"description": "All products layout for StorePress.",
	"supports": {
		"html": false
	},
	"attributes": {
		"gridColumns": {
			"type": "number",
			"default": 1
		},
		"gridRows": {
			"type": "number",
			"default": 1
		},
		"gridGap": {
			"type": "number",
			"default": 1
		}
	},
	"textdomain": "sp-all-products",
	"editorScript": "file:../../build/product-layout.js",
	"editorStyle": "file:../../build/product-layout.css",
	"style": "file:../../build/style-product-layout.css"
}
