{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "store-press/sp-all-products",
	"version": "1.0.0",
	"title": "SP All Products",
	"category": "sp-block-category",
	"icon": "layout",
	"description": "All products layout for StorePress.",
	"supports": {
		"html": false
	},
	"attributes": {
		"gridColumns": {
			"type": "number",
			"default": 2
		},
		"gridRows": {
			"type": "number",
			"default": 2
		},
		"gridGap": {
			"type": "number",
			"default": 10
		},
		"productCategories": {
			"type": "string",
			"default": null
		},
		"productOffset": {
			"type": "number",
			"default": 0
		},
		"orderBy": {
			"type": "string",
			"default": ""
		},
		"order": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "sp-all-products",
	"editorScript": "file:../../../build/product-layout.js",
	"editorStyle": "file:../../../build/product-layout.css",
	"style": "file:../../../build/style-product-layout.css"
}
